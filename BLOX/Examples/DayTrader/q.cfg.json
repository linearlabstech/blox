{
    "Nets":{
        "DayTrader":"q.net.json"
    },
    "Load":{
        "DayTrader":"DayTrader.pt"
    },
    "Train":{
        "Optimizer":{
            "Params":[
                "DayTrader"
            ],
            "Kwargs":{
                "lr":0.00001,"weight_decay":0.995
            },
            "Algo":"DQNAdam"
        },
        "Loss":{
            "Algo":"SmoothL1Loss",
            "Kwargs":{}
        },

        "Epochs":15,
        "SaveEvery":5000,
        "Environment":{
            "Class":"TradingEnvironment",
            "Kwargs":{
                "fname":"trader.ds"   
            }
        },
        "FileExt":".pt",
        "Verbose":false,
        "TensorboardX":{
            "LogEvery":25,
            "Dir":"runs/",
            "SaveGraphs":false,
            "Log":[
                "gainz","value","n_shares","wallet","loss"
            ]
        }
    },
    "Pipeline":{
        "threads":1,
        "addr":"localhost",
        "queue":"default",
        "Pipes":[
            {
                "Nets":{
                    "DayTrader":"net.json"
                }
            }
        ]
    },
    "Deploy":[
        {
            "url":"/predict",
            "name":"predict",
            "client":{},
            "arg":"data"
        }
    ],
    "Process":[
        "Train",
        "Pipeline"
    ]
    
}