{
    "Nets":{
        "DayTrader":"net.json"
    },
    "Train":{
        "Optimizer":{
            "Params":[
                "DayTrader"
            ],
            "Kwargs":{
                "lr":0.00001
            },
            "Algo":"Adam"
        },
        "Loss":{
            "Algo":"CrossEntropyLoss",
            "Kwargs":{}
        },
        "Epochs":25,
        "SaveEvery":500,
        "DataSet":"trader.ds",
        "FileExt":".pt",
        "Verbose":true,
        "TensorboardX":{
            "LogEvery":125,
            "Dir":"runs/",
            "SaveGraphs":false,
            "Log":[
                "Loss"
            ]
        }
    },
    "Load":{
        "DayTrader":"DayTrader-.pt"
    },
    "Pipeline":{
        "threads":1,
        "addr":"localhost",
        "queue":"default",
        "Pipes":[
            {
                "Nets":{
                    "DayTrader":"net.json"
                }
            }
        ]
    },
    "Deploy":[
        {
            "url":"/predict",
            "name":"predict",
            "client":{},
            "arg":"data"
        }
    ],
    "Process":[
        "Train",
        "Pipeline"
    ]
    
}