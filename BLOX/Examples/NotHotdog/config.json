{
    "Nets":{
        "NotHotdog":"net.json"
    },
    "Train":{
        "Optimizer":{
            "Params":[
                "NotHotdog"
            ],
            "Kwargs":{
                "lr":0.001
            },
            "Algo":"SGD"
        },
        "Loss":{
            "Algo":"L1Loss",
            "Kwargs":{}
        },
        "Epochs":50,
        "SaveEvery":50,
        "DataSet":"imgs.ds",
        "FileExt":".pt",
        "Verbose":true,
        "TensorboardX":{
            "LogEvery":10,
            "Dir":"runs/",
            "SaveGraphs":false,
            "Log":[
                "Loss"
            ]
        }
    },
    "Load":{
        "NotHotdog":"NotHotdog-.pt"
    },
    "Pipeline":{
        "threads":1,
        "addr":"localhost",
        "queue":"default",
        "Pipes":[
            {
                "Nets":{
                    "NotHotdog":"net.json"
                }
            }
        ]
    },
    "Data":{
        "Convert":{
            "Img":{
                "LoadDir":"downloads",
                "SaveTo":"imgs.ds"
            }
        }
    },
    "Deploy":[
        {
            "url":"/predict",
            "name":"predict",
            "client":{},
            "arg":"data"
        }
    ],
    "Process":[
        "Data",
        "Train",
        "Pipeline"
    ]
    
}